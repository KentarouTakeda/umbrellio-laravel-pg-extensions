{
  "name": "umbrellio/laravel-postgres-driver",
  "type": "library",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Vitaliy Lazeev",
      "email": "vetal@umbrellio.biz"
    }
  ],
  "require": {
    "php": "^7.3",
    "laravel/framework": "^5.8"
  },
  "require-dev": {
    "umbrellio/code-style-php": "^1.0",
    "orchestra/testbench": "^3.5"
  },
  "scripts": {
    "lint": [
      "ecs check --config=ecs.yml . --fix"
    ]
  },
  "autoload": {
    "psr-4": {
      "Umbrellio\\Postgres\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Umbrellio\\Postgres\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Umbrellio\\Postgres\\PostgresDatabaseServiceProvider"
      ]
    }
  }
}
